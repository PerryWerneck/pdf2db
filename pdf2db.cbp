<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Extract data from PDF to SQL Database" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output=".bin/Debug/pdfimporter" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++1z" />
					<Add option="-g" />
					<Add option="-DDEBUG=1" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output=".bin/Release/pdfimporter" prefix_auto="1" extension_auto="1" />
				<Option object_output=".obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG=1" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="`pkg-config --cflags poppler-cpp`" />
			<Add option="-DAPPNAME=PwPET" />
			<Add directory="src/components" />
			<Add directory="src/include" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config --libs poppler-cpp`" />
			<Add library="pugixml" />
			<Add library="cppdb" />
		</Linker>
		<Unit filename="src/include/config.h" />
		<Unit filename="src/include/pdfdb.h" />
		<Unit filename="src/main/contents.cc" />
		<Unit filename="src/main/document.cc" />
		<Unit filename="src/main/filters.cc" />
		<Unit filename="src/main/parser.cc" />
		<Unit filename="src/main/pdfdb.cc" />
		<Unit filename="src/main/query.cc" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
