		<Unit filename="src/components/core/string/string.cc" />
		<Unit filename="src/components/core/string/test.cc" />
		<Unit filename="src/components/core/string/tostring.cc" />
		<Unit filename="src/components/core/stringtable.cc" />
		<Unit filename="src/components/core/tempfile.cc" />
		<Unit filename="src/components/core/textfile.cc" />
		<Unit filename="src/components/core/windows/registry.cc" />
		<Unit filename="src/include/config.h" />
		<Unit filename="src/include/pdfimporter.h" />
		<Unit filename="src/main/contents.cc" />
		<Unit filename="src/main/document.cc" />
		<Unit filename="src/main/filters.cc" />
